
<ul class="list-inline">
<li><%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Create account.", new_registration_path(resource_name), class: "login-links" %>
<% end -%></li>

<li><%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Forgot your password?", new_password_path(resource_name), class: "login-links" %>
<% end -%></li>

<li><%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class: "login-links" %>
<% end -%></li>

<li><%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), class: "login-links" %>
<% end -%></li>
</ul>
